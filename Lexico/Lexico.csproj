<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8</LangVersion>
    <Nullable>enable</Nullable>
    
    <PackageId>Lexico</PackageId>
    <Version>1.0.0-alpha5</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>hamish-milne, Craig-J</Authors>
    <PackageDescription>Type-based recursive descent parser for .NET. Lexico generates the parse grammar using attributes and produces object instances when applied.</PackageDescription>
    <PackageTags>Parser; Parsing</PackageTags>
    <RepositoryUrl>https://github.com/hamish-milne/Lexico</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
